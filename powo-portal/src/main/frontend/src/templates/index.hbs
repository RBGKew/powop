{{> partials/header }}
<div id="svgs" style="display:none">
  {{> svg/leaf.svg }}
  {{> svg/mushroom.svg }}
  {{> svg/chevron.svg }}
  {{> (concat "svg/" siteId ".svg")}}
</div>
<div class="s-page s-page--home s-search__fullpage {{concat " s-" siteId}} invisible">
  <div class="s-page-wrapper">
    {{> partials/page-header}}

    <div class="c-search autocomplete-form">
      <div class="container front-page-container">
        <div class="central">
          <div class="c-intro">
            {{> (concat "partials/intro/" siteId) }}
          </div>
          <main id="main" class="front-page" tabindex="-1">
            {{> partials/search/search-box }}
          </main>
        </div>
        <aside class="featured">
          {{> partials/featured}}
        </aside>
      </div><!-- End Container -->
    </div>
    <div class="featured-taxa-sections container">
      {{#each featured-taxa-sections}}
      <header>
      <h2>{{title}}</h2>
      <div class="view-all">
        <a href="/?q=">View All</a>
      </div>
      </header>
      {{#each featuredTaxa}}
        <div class="col-12 col-md-4 {{#if @first}}first{{/if}} {{#if @last}} last{{/if}}">
          {{> partials/featured-taxon }}
        </div>
      {{/each}}
      {{/each}}
    </div>
    {{> partials/footer-links}}
  </div>
</div>
<script>
  window.siteData = {
    basePageTitle: "{{ title }}"
  }
</script>
{{> partials/footer initialize="search"}}