{{> partials/header }}
<div id="svgs" style="display:none">
  {{> svg/leaf.svg }}
  {{> svg/mushroom.svg }}
  {{> svg/chevron.svg }}
  {{> (concat "svg/" siteId ".svg")}}
</div>
<div class="s-page s-page--home s-search__fullpage {{concat " s-" siteId}}">
  <div class="s-page-wrapper">
    {{> partials/page-header}}
    <main id="#main">
      <div class="main-content home__hero-content">
        <div class="central">
          <div class="central-items">
            <div class="c-intro">
              {{> (concat "partials/intro/" siteId) }}
            </div>
            <div class="home__search-container" tabindex="-1">
              {{> partials/search/search-box }}
            </div>
            <div class="form-inline--search">
              {{> (concat "partials/cross-site-help/cross-site-help-" siteId) }}
            </div>
          </div>
        </div>
        {{> (concat "partials/featured/featured-image-description-" siteId)}}        
      </div>
      <div class="featured-taxa-sections">
        {{> partials/featured/featured-taxa-sections}}
      </div>
    </main>
    {{> partials/footer-links}}
  </div>
</div>
<script>
  window.siteData = {
    basePageTitle: "{{ title }}"
  }
</script>
{{> partials/footer initialize="static"}}